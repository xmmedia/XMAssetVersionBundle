    "type": "symfony-bundle",
    "homepage": "https://github.com/xmmedia/XMAssetVersionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Hein",
            "email": "dhein@xmmedia.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/asset": "~3.0"
    },
    "autoload": {
        "psr-4": { "XM\\AssetVersionBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
